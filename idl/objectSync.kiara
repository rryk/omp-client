namespace * omp;

struct Vec3f {
  float X, Y, Z;
}

struct Quatf {
  float W, Z, X, Y;
}

service objectSync {
   [Oneway] void createObject(u32 id, string xml3dRepresentation);
   [Oneway] void deleteObject(u32 id);
   [Oneway] void locationUpdate(u32 id, Vec3f position, Quatf rotation, Vec3f scale);
   // void movementUpdate(u32 id, Vec3f velocity, Vec3f acceleration, Vec3f angularVelocity);
}