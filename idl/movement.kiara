namespace * omp;

struct Vec3f {
  float X, Y, Z;
}

struct Quatf {
  float W, Z, X, Y;
}

service movement {
   [Oneway] void updateAvatarLocation(string uuid, Vec3f position, Quatf rotation, Vec3f scale);
   [Oneway] void updateAvatarMovement(string uuid, Vec3f velocity, Vec3f acceleration, Vec3f angularVelocity);
}